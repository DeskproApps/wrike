{
  "name": "@deskpro-apps/wrike",
  "title": "Wrike",
  "description": "Wrike integration for Deskpro",
  "version": "0.0.0",
  "scope": "agent",
  "isSingleInstall": false,
  "hasDevMode": true,
  "targets": [
    {
      "target": "ticket_sidebar",
      "entrypoint": "index.html"
    }
  ],
  "settings": {
    "access_token": {
      "title": "Access Token",
      "description": "Example backend only setting",
      "type": "string",
      "isRequired": true,
      "isBackendOnly": true
    }
  },
  "entityAssociations": {
    "linkedWrikeTasks": {
      "entity": "ticket",
      "type": "external_id",
      "title": "Linked Wrike Tasks"
    }
  },
  "proxy": {
    "whitelist": [
      {
        "url": "https://www.wrike.com/.*",
        "methods": [
          "GET",
          "POST",
          "PUT"
        ],
        "timeout": 20
      }
    ]
  }
}